FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        system;
    object          probesDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
type probes;


// Fields to be probed. runTime modifiable!
fields
(
    U
);

// Locations to be probed. runTime modifiable!
probeLocations
(
    (10.0 0.0 0.0)
);

